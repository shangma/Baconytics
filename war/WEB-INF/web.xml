<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Baconytics</servlet-name>
		<servlet-class>edu.gatech.cc.Baconytics.BaconyticsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>scraper</servlet-name>
		<servlet-class>edu.gatech.cc.Baconytics.Scraper.ScraperServlet</servlet-class>
	</servlet>
	<servlet>
        <servlet-name>testfeeder</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.FeederTester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testapi</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.NlpApiTester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testextractor</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.ExtractorTester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testputter</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.PutterTester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>initkeyword</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.InitKeywordEntity</servlet-class>
    </servlet>
        <servlet>
        <servlet-name>aggregator</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.AggregatorTester</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>show</servlet-name>
        <servlet-class>test.edu.gatech.cc.Baconytics.appengine.Show</servlet-class>
    </servlet>

    
    <servlet-mapping>
		<servlet-name>Baconytics</servlet-name>
		<url-pattern>/baconytics</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>scraper</servlet-name>
		<url-pattern>/scraper</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
        <servlet-name>testfeeder</servlet-name>
        <url-pattern>/testfeeder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testapi</servlet-name>
        <url-pattern>/testapi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testextractor</servlet-name>
        <url-pattern>/testextractor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testputter</servlet-name>
        <url-pattern>/testputter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>initkeyword</servlet-name>
        <url-pattern>/initkeyword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aggregator</servlet-name>
        <url-pattern>/aggregator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>show</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>
    
    <welcome-file-list>
		<welcome-file>baconytics</welcome-file>
	</welcome-file-list>
</web-app>
